---
kind: ObjectType
version: v1
definition:
  name: ConsumerLendingLoanApplications
  description: Stores consumer lending applications for non-mortgage loans
  fields:
    - name: applicationChannel
      type: ConsumerLendingVarchar
      description: Online, Mobile, Branch, Phone
    - name: applicationType
      type: ConsumerLendingVarchar!
      description: Personal, Auto, Student, Home Improvement, etc.
    - name: branchId
      type: ConsumerLendingInt4
      description: Branch where application was processed
    - name: consumerLendingApplicationId
      type: ConsumerLendingInt4!
      description: Unique identifier for the loan application
    - name: creationDateTime
      type: Timestamp!
      description: When application was initially created
    - name: customerId
      type: ConsumerLendingInt4!
      description: Reference to enterprise.accounts
    - name: decisionDateTime
      type: Timestamp
      description: When final decision was made
    - name: decisionReason
      type: ConsumerLendingVarchar
      description: Primary reason for approval/denial
    - name: estimatedCreditScore
      type: ConsumerLendingInt4
      description: Estimated or self-reported credit score
    - name: lastUpdatedDateTime
      type: Timestamp!
      description: Last modification timestamp
    - name: loanPurpose
      type: ConsumerLendingVarchar!
      description: Purpose for the loan
    - name: officerId
      type: ConsumerLendingInt4
      description: Loan officer assigned to application
    - name: referralSource
      type: ConsumerLendingVarchar
      description: Marketing channel or referral source
    - name: requestedAmount
      type: ConsumerLendingNumeric!
      description: Amount requested by applicant
    - name: requestedTermMonths
      type: ConsumerLendingInt4!
      description: Loan term in months requested by applicant
    - name: status
      type: ConsumerLendingVarchar!
      description: Draft, Submitted, In Review, Approved, Denied, Cancelled
    - name: submissionDateTime
      type: Timestamp
      description: When application was submitted by customer
  graphql:
    typeName: ConsumerLendingLoanApplications
    inputTypeName: ConsumerLendingLoanApplicationsInput
  dataConnectorTypeMapping:
    - dataConnectorName: consumer_lending
      dataConnectorObjectType: consumer_lending_loan_applications
      fieldMapping:
        applicationChannel:
          column:
            name: application_channel
        applicationType:
          column:
            name: application_type
        branchId:
          column:
            name: branch_id
        consumerLendingApplicationId:
          column:
            name: consumer_lending_application_id
        creationDateTime:
          column:
            name: creation_date_time
        customerId:
          column:
            name: customer_id
        decisionDateTime:
          column:
            name: decision_date_time
        decisionReason:
          column:
            name: decision_reason
        estimatedCreditScore:
          column:
            name: estimated_credit_score
        lastUpdatedDateTime:
          column:
            name: last_updated_date_time
        loanPurpose:
          column:
            name: loan_purpose
        officerId:
          column:
            name: officer_id
        referralSource:
          column:
            name: referral_source
        requestedAmount:
          column:
            name: requested_amount
        requestedTermMonths:
          column:
            name: requested_term_months
        status:
          column:
            name: status
        submissionDateTime:
          column:
            name: submission_date_time

