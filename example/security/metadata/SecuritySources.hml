---
kind: Relationship
version: v1
definition:
  name: securityEntitlements
  sourceType: SecuritySources
  target:
    model:
      name: SecurityEntitlements
      relationshipType: Array
      aggregate:
        aggregateExpression: SecurityEntitlementsAggExp
  mapping:
